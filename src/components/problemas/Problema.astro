---
import { contenidoCards } from "@/scripts/contenidoCards";
const { id } = Astro.params;

const problema = contenidoCards.find((problema) => problema.id === Number(id));
---

<section>
    <div class="container">
        <div class="row">
            <div class="col-12">
                <h1>{problema.titulo}</h1>
                <p>{problema.imagen}</p>
            </div>
        </div>
    </div>
</section>
